Create Database BigData;


use BigData;




drop database BigData;




create database BigData;


use BigData;



create table employees
( id int NOT NULL, 
name VARCHAR(50) NOT NULL,
hiring_date DATE DEFAULT "2021-02-08",
salary DOUBLE,
UNIQUE(id),
CHECK (salary>500 )
);



insert into employees values(1,"a","1999-08-08" , 800);
insert into employees values(2,"b","1999-08-08" , 800);



SELECT * from employees
